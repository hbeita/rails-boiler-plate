<p id="notice"><%= notice %></p>

<h1>Church Services</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @membership_church_services.each do |membership_church_service| %>
      <tr>
        <td><%= membership_church_service.date %></td>
        <td><%= link_to 'Show', membership_church_service %></td>
        <td><%= link_to 'Edit', edit_membership_church_service_path(membership_church_service) %></td>
        <td><%= link_to 'Destroy', membership_church_service, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

